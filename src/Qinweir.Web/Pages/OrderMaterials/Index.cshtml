@page
@inherits Qinweir.Web.Pages.QinweirPage
@model Qinweir.Web.Pages.OrderMaterials.IndexModel


@{
    ViewData["Title"] = "Index";
}

<h1>订单管理页面</h1>

@section scripts
{
    <abp-script src="/Pages/OrderMaterials/index.js" />
}
<abp-card>
    <abp-card-header>
        <h2>@L["Books"]</h2>
    </abp-card-header>
    <abp-card-body>
        <abp-table striped-rows="true" id="BooksTable">
            <thead>
                <tr>
                    <th>@L["Id"]</th>
                    <th>@L["OrderStore"]</th>
                    <th>@L["OrderTime"]</th>
                    <th>@L["MaterialsType"]</th>
                    <th>@L["MaterialsName"]</th>
                    <th>@L["units"]</th>
                    <th>@L["MateriralsCount"]</th>
                    <th>@L["MateriralsPrice"]</th>
                    <th>@L["Remark"]</th>
                </tr>
            </thead>
        </abp-table>
    </abp-card-body>
</abp-card>